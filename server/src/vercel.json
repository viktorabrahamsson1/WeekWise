{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@now/node"
    },
    {
      "src": "client/main.tsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
